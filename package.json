  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.2.2",
    "@angular/compiler": "^2.2.2",
    "@angular/compiler-cli": "^2.2.4",
    "@angular/core": "^2.2.2",
    "@angular/forms": "^2.2.2",
    "@angular/http": "^2.2.2",
    "@angular/platform-browser": "^2.2.2",
    "@angular/platform-browser-dynamic": "^2.2.2",
    "@angular/platform-server": "^2.2.4",
    "@angular/router": "^3.2.2",
    "@angular/upgrade": "^2.2.2",
    "angular-in-memory-web-api": "^0.1.16",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "^0.19.41",
    "zone.js": "^0.6.26"
  },
