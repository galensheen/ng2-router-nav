  "devDependencies": {
    "@types/node": "^0.0.2",
    "concurrently": "^3.1.0",
    "del": "^2.2.2",
    "jspm": "^0.16.48",
    "lite-server": "^2.2.2",
    "mkdirp": "^0.5.1",
    "rollup": "^0.36.4",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "typescript": "^2.0.10"
  }
