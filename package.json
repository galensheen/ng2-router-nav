{
  "name": "ng2-router-nav",
  "version": "1.0.0",
  "description": "angular2 router and navigation",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\"",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js",
    "build": "node aot-pre.js && npm run ngc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/galensheen/ng2-router-nav.git"
  },
